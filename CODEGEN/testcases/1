PROGRAM aa(input, output, error); 
VAR a, b: INTEGER;
VAR c, d: REAL;

FUNCTION simplesum(a, b:integer) : integer;
	VAR e : integer;
	begin
		e := 1 + b;
		simplesum := a + e
	end;


BEGIN
    a := 1 * 3 + 9;
	b := 2 * a + 4 / 2 - a;
	d := 2.3 * 2.9 + 7.5;
	c := 3.1 / 2.0 * d;

	if a < b
			then b := b + 1
			else b := a + 1;

	while simplesum(1, 2) == 4
			DO a := a;

END.
